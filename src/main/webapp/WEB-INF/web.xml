<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>ControladorListarRegistro</servlet-name>
        <servlet-class>web.ControladorListarRegistro</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorMenuAdmin</servlet-name>
        <servlet-class>web.ControladorMenuAdmin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorPostulante</servlet-name>
        <servlet-class>web.ControladorPostulante</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ControladorListarRegistro</servlet-name>
        <url-pattern>/ControladorListarRegistro</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorMenuAdmin</servlet-name>
        <url-pattern>/ControladorMenuAdmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorPostulante</servlet-name>
        <url-pattern>/ControladorPostulante</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
